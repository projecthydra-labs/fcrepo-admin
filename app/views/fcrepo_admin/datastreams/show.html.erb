<h1><%= @object.pid %></h1>

<h3>Datastream: <%= @datastream.dsid %></h3>

<div class="document">

  <div class="tabbable">
    <ul class="nav nav-tabs">
      <li class="active"><a href="#tab1" data-toggle="tab">Content</a></li>
      <li><a href="#tab2" data-toggle="tab">Profile</a></li>
    </ul>
    <div class="tab-content">
      <div class="tab-pane active" id="tab1">
	<%= render :partial => 'content', :locals => {:datastream => @datastream, :inline => @inline} %>
      </div>
      <div class="tab-pane" id="tab2">
	<%= render :partial => 'profile_table', :locals => {:datastream => @datastream} %>
      </div>
    </div>
  </div>

</div>
